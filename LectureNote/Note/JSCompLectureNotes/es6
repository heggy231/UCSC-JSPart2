const foo = 1;

console.log(foo);

const bar = {};

bar.name= "bar";
bar.title = "arch";

console.log(bar.name);

// bar = 1; // bar is const therefore you can't reassign
console.log(bar);

const myArr = [];
myArr.push(123);
console.log(myArr);

// myArr = {}

function add(a,b){
    return a + b;
}
add("hi", "cutie");
console.log (add("hi", "cutie"));
console.log (add(2,10));